<div class="private-square span4 text-center">
  
  <h1>Upload your Image</h1>
  <br/>
  <%= simple_form_for @post do |f| %>
  <%= f.input :title, placeholder: 'eg. puppies' %>
</div>
<div class="clr">
</div>
<br />
<div class="private-square span4 text-center">
  <%= f.input :description, placeholder: 'eg. puppies are adorable' %>
  
  
  <%= f.button :submit, :class => 'btn btn-success' %>
  </div>
  <% end %>
  